  "ayahs": [
    {
      "no": 1,
      "arab": "سَبِّحِ اسْمَ رَبِّكَ الْاَعْلَىۙ ",
      "latin": "sabbi<u>h</u>i isma rabbika <strong>a</strong>l-a'l<u>aa</u>",
      "translation": "Sucikanlah nama Tuhanmu Yang Mahatinggi,",
