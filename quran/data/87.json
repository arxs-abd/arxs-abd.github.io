        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/5949.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/5949.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/5949.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/5949.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/5949.mp3"
      }
    },
    {
      "no": 2,
      "arab": "الَّذِيْ خَلَقَ فَسَوّٰىۖ ",
      "latin": "<strong>al</strong>la<u>dz</u>ii khalaqa fasaww<u>aa</u>",
      "translation": "Yang menciptakan, lalu menyempurnakan (ciptaan-Nya).",
