  "ayahs": [
    {
      "no": 1,
      "arab": "وَالشَّمْسِ وَضُحٰىهَاۖ",
      "latin": "wa<strong>al</strong>sysyamsi wa<u>dh</u>u<u>haa</u>h<u>aa</u>",
      "translation": "Demi matahari dan sinarnya pada pagi hari,",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6044.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6044.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6044.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6044.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6044.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6044.mp3"
      }
    },
    {
      "no": 2,
      "arab": "وَالْقَمَرِ اِذَا تَلٰىهَاۖ",
      "latin": "wa<strong>a</strong>lqamari i<u>dzaa</u> tal<u>aa</u>h<u>aa</u>",
      "translation": "demi bulan apabila mengiringinya,",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6045.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6045.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6045.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6045.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6045.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6045.mp3"
      }
    },
    {
      "no": 3,
      "arab": "وَالنَّهَارِ اِذَا جَلّٰىهَاۖ",
      "latin": "wa<strong>al</strong>nnah<u>aa</u>ri i<u>dzaa</u> jall<u>aa</u>h<u>aa</u>",
      "translation": "demi siang apabila menampakkannya,",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6046.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6046.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6046.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6046.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6046.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6046.mp3"
      }
    },
    {
      "no": 4,
      "arab": "وَالَّيْلِ اِذَا يَغْشٰىهَاۖ",
      "latin": "wa<strong>a</strong>llayli i<u>dzaa</u> yaghsy<u>aa</u>h<u>aa</u>",
      "translation": "demi malam apabila menutupinya (gelap gulita),",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6047.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6047.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6047.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6047.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6047.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6047.mp3"
      }
    },
    {
      "no": 5,
      "arab": "وَالسَّمَاۤءِ وَمَا بَنٰىهَاۖ",
      "latin": "wa<strong>al</strong>ssam<u>aa</u>-i wam<u>aa</u> ban<u>aa</u>h<u>aa</u>",
      "translation": "demi langit serta pembinaannya (yang menakjubkan),",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6048.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6048.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6048.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6048.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6048.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6048.mp3"
      }
    },
    {
