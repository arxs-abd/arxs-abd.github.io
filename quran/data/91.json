      }
    },
    {
      "no": 7,
      "arab": "وَنَفْسٍ وَّمَا سَوّٰىهَاۖ",
      "latin": "wanafsin wam<u>aa</u> saww<u>aa</u>h<u>aa</u>",
      "translation": "demi jiwa serta penyempurnaan (ciptaan)nya,",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6050.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6050.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6050.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6050.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6050.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6050.mp3"
      }
    },
    {
      "no": 8,
      "arab": "فَاَلْهَمَهَا فُجُوْرَهَا وَتَقْوٰىهَاۖ",
      "latin": "fa-alhamah<u>aa</u> fujuurah<u>aa</u> wataqw<u>aa</u>h<u>aa</u>",
      "translation": "maka Dia mengilhamkan kepadanya (jalan) kejahatan dan ketakwaannya,",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6051.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6051.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6051.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6051.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6051.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6051.mp3"
      }
    },
    {
      "no": 9,
      "arab": "قَدْ اَفْلَحَ مَنْ زَكّٰىهَاۖ",
      "latin": "qad afla<u>h</u>a man zakk<u>aa</u>h<u>aa</u>",
      "translation": "sungguh beruntung orang yang menyucikannya (jiwa itu),",
      "audio": {
        "alafasy": "https://cdn.islamic.network/quran/audio/128/ar.alafasy/6052.mp3",
        "ahmedajamy": "https://cdn.islamic.network/quran/audio/128/ar.ahmedajamy/6052.mp3",
        "husarymujawwad": "https://cdn.islamic.network/quran/audio/128/ar.husarymujawwad/6052.mp3",
        "minshawi": "https://cdn.islamic.network/quran/audio/128/ar.minshawi/6052.mp3",
        "muhammadayyoub": "https://cdn.islamic.network/quran/audio/128/ar.muhammadayyoub/6052.mp3",
        "muhammadjibreel": "https://cdn.islamic.network/quran/audio/128/ar.muhammadjibreel/6052.mp3"
      }
    },
